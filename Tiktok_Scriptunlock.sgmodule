#!name=tiktok脚本解锁去水印
#!desc=解锁去水印，script下修改代码
#!system=ios

[Script]


#下面两个脚本是解锁和去水印一起的，如果要启用需要把上面脚本注释掉，把[URL Rewrite]段删除
tiktok=type=http-request,pattern=^https:\/\/.+(tiktokv|byteoversea)\.com\/.+\=CN.+,requires-body=0,script-path=Script/123.js

tiktok去水印=type=http-response,pattern=^https?:\/\/.*\.tiktokv\.com\/aweme\/v\d\/((follow\/)?feed|(((multi\/)?aweme\/))?(favorite|post|detail)),requires-body=1,script-path=Script/123.js



[MITM]
hostname = %APPEND% *.byteoversea.com,*.tiktokv.com
